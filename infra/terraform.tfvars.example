# General resource settings
resource_group_name = "rso-dev-rg"
location            = "Germany West Central"

# Container registry
acr_name = "rsobingusiacr"
acr_sku  = "Basic"

# Postgres flexible server
pg_name       = "rsobingusipgs"
pg_version    = "16"
pg_sku        = "B_Standard_B1ms"
pg_storage_mb = 32768
pg_location   = "Switzerland North"

# Kubernetes service
aks_name = "rsobingusiaks"
aks_node_count = 1
aks_node_vm_size = "Standard_B2pls_v2"

# Key Vault
keyvault_name = "rsobingusivault"
